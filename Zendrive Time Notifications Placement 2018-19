#include<bits/stdc++.h>
using namespace std;
typedef long long int64;
int to_int(string s){
    stringstream ss(s);
    int x ;
    ss >> x;
    return x;
}
vector<int> time(string s){
    int i = 0; int n = s.size();
    while(i < n && s[i] != ':'){m
        i++;
    }
    vector<int>v(3);
    v[0] = to_int(s.substr(0, i));
    int i1 = i; i++;
    while(i < n && s[i] != ':'){
        i++;
    }
    v[1] = to_int(s.substr(i1 + 1, i - i1 + 1));
    v[2] = to_int(s.substr(i + 1));
    return v;
}
int main(){
    string s; cin >> s;
    vector<int> t = time(s);
    for(int i = 0; i < 3; i++){
            cout << t[i] << " ";
        }
        cout << endl;
    int T; cin >> T;
    while(T--){
        string x; cin >> x;
        vector<int> v = time(x);
        for(int i = 0; i < 3; i++){
            cout << v[i] << " ";
        }
        bool timeDiff0 = true;
        // timediff 0
        for(int i = 0; i < 3; i++){
            if(v[i] != t[i]){
                timeDiff0 = false;
                break;
            }
        }
        if(timeDiff0){
            cout << "now" << endl ;
        }
        else{
        for(int i = 0; i < 3; i++){
            int diff = t[i] - v[i];
            if(v[i] != t[i]){
                if(i == 0){
                    if(diff == 1){
                        cout << diff << " hour ago" << endl;
                    }
                    else{
                        cout << diff << " hours ago" << endl;
                    }
                    break;
                }
                else if(i == 1){
                    if(diff == 1){
                        cout << diff << " minute ago" << endl;
                    }
                    else{
                        cout << diff << " minutes ago" << endl;
                    }
                    break;
                }
                else if(i == 2){
                    if(diff == 1){
                        cout << diff << " second ago" << endl;
                    }
                    else{
                        cout << diff << " seconds ago" << endl;
                    }
                }
            }
        }
        }
    }
    return 0;
}
